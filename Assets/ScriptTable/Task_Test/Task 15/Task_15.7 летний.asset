      \u043B\u044E\u043A\u0438 \u0441 \u043F\u044B\u043B\u0435\u0444\u0438\u043B\u044C\u0442\u0440\u0430\u043C\u0438
      (\u043B\u044E\u043A\u0438 2, 3 \u0438 4), \u0432\u044B\u0442\u044F\u0436\u043D\u043E\u0439
      \u043B\u044E\u043A \u043D\u0430 \u043A\u0440\u044B\u0448\u0435 \u043A\u0443\u0437\u043E\u0432\u0430
      \u0438 \u043A\u0440\u044B\u0448\u043A\u0438 \u043D\u0430 \u043A\u043E\u0436\u0443\u0445\u0435
      \u0432\u044B\u0442\u044F\u0436\u043D\u044B\u0445 \u0432\u0435\u043D\u0442\u0438\u043B\u044F\u0442\u043E\u0440\u043E\u0432.
      \u0417\u0430\u0441\u043B\u043E\u043D\u043A\u0438 \u043D\u0430 \u0432\u0435\u043D\u0442\u0438\u043B\u044F\u0446\u0438\u043E\u043D\u043D\u044B\u0445
      \u044F\u0449\u0438\u043A\u0430\u0445 \u0432\u043D\u0443\u0442\u0440\u0438 \u043A\u0443\u0437\u043E\u0432\u0430
      \u0437\u0430\u043A\u0440\u043E\u0439\u0442\u0435.\n\n\u0412\u043A\u043B\u044E\u0447\u0438\u0442\u0435
