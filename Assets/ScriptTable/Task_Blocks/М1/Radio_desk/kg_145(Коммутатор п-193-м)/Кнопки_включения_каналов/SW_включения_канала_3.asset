    \u0434\u043B\u044F \u043E\u0440\u0433\u0430\u043D\u0438\u0437\u0430\u0446\u0438\u0438
    \u0441\u0432\u044F\u0437\u0438 \u0432 \u0432\u043E\u0435\u043D\u043D\u044B\u0445
    \u0443\u0441\u043B\u043E\u0432\u0438\u044F\u0445. \u041E\u043D \u043E\u0431\u0435\u0441\u043F\u0435\u0447\u0438\u0432\u0430\u0435\u0442
    \u043F\u043E\u0434\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u0435 \u0434\u043E
    10 \u0430\u0431\u043E\u043D\u0435\u043D\u0442\u0441\u043A\u0438\u0445 \u043B\u0438\u043D\u0438\u0439
    \u0441 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C\u044E
    \u0440\u0430\u0441\u0448\u0438\u0440\u0435\u043D\u0438\u044F \u0434\u043E 20
    \u043D\u043E\u043C\u0435\u0440\u043E\u0432 \u043F\u0440\u0438 \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u043D\u0438\u0438
    \u0434\u0432\u0443\u0445 \u0443\u0441\u0442\u0440\u043E\u0439\u0441\u0442\u0432.\r\n\u0401\u043C\u043A\u043E\u0441\u0442\u044C
    \u0441\u0442\u0430\u043D\u0446\u0438\u0438: 10 \u0430\u0431\u043E\u043D\u0435\u043D\u0442\u0441\u043A\u0438\u0445
    \u043B\u0438\u043D\u0438\u0439 (2-\u043F\u0440\u043E\u0432\u043E\u0434\u043D\u044B\u0435
    \u043A\u0430\u0431\u0435\u043B\u044C\u043D\u044B\u0435 \u0438\u043B\u0438 \u0432\u043E\u0437\u0434\u0443\u0448\u043D\u044B\u0435),
    \u0440\u0430\u0441\u0448\u0438\u0440\u044F\u0435\u0442\u0441\u044F \u0434\u043E
    20 \u043B\u0438\u043D\u0438\u0439 \u043F\u0440\u0438 \u0441\u043F\u0430\u0440\u0438\u0432\u0430\u043D\u0438\u0438
    \u0434\u0432\u0443\u0445 \u043A\u043E\u043C\u043C\u0443\u0442\u0430\u0442\u043E\u0440\u043E\u0432.\r\n\u042D\u043B\u0435\u043A\u0442\u0440\u043E\u043F\u0438\u0442\u0430\u043D\u0438\u0435:
    \u0411\u0430\u0442\u0430\u0440\u0435\u044F \u0413\u0411-10-\u0423-1,3 (9 \u0412
    \xB1 1,5 \u0412).\r\n\u0417\u0430\u0449\u0438\u0442\u0430: \u041E\u0442 \u0442\u043E\u043A\u043E\u0432
    \u0432\u044B\u0441\u043E\u043A\u043E\u0433\u043E \u043D\u0430\u043F\u0440\u044F\u0436\u0435\u043D\u0438\u044F
    (\u0433\u0440\u043E\u0437\u043E\u0432\u044B\u0435 \u0440\u0430\u0437\u0440\u044F\u0434\u044B,
    \u041B\u042D\u041F)2.\r\n\u041A\u043E\u0440\u043F\u0443\u0441: \u041C\u0435\u0442\u0430\u043B\u043B\u0438\u0447\u0435\u0441\u043A\u0438\u0439
    \u044F\u0449\u0438\u043A \u0441 \u043F\u0435\u0440\u0435\u0434\u043D\u0435\u0439
    \u0438 \u0432\u0435\u0440\u0445\u043D\u0435\u0439 \u0434\u0432\u0435\u0440\u0446\u0430\u043C\u0438
    \u0434\u043B\u044F \u0437\u0430\u0449\u0438\u0442\u044B \u043E\u0442 \u043F\u043E\u0432\u0440\u0435\u0436\u0434\u0435\u043D\u0438\u0439
    \u0438 \u0437\u0430\u0433\u0440\u044F\u0437\u043D\u0435\u043D\u0438\u0439. \u041E\u0441\u043D\u0430\u0449\u0451\u043D
