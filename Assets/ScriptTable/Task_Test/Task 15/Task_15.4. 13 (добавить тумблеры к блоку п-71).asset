      \u0442\u0443\u043C\u0431\u043B\u0435\u0440 \u041E\u0422\u041C.-\u041E\u0422\u041A\u041B.
      \u0412 \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u041E\u0422\u041C.
      \u0438, \u0432\u0440\u0430\u0449\u0430\u044F \u0448\u043B\u0438\u0446\u044B
      \u0423\u0421\u0418\u041B. \u041E\u0422\u041C. \u0414\u0418\u0421\u0422. \u0438
      \u0423\u0421\u0418\u041B. \u041E\u0422\u041C. \u0410\u0417\u0418\u041C., \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435
      \u0447\u0435\u0442\u043A\u0443\u044E \u0432\u0438\u0434\u0438\u043C\u043E\u0441\u0442\u044C
      \u043E\u0442\u043C\u0435\u0442\u043E\u043A \u0434\u0438\u0441\u0442\u0430\u043D\u0446\u0438\u0438
      \u0438 \u0430\u0437\u0438\u043C\u0443\u0442\u0430 (50 \u043A\u0438\u043B\u043E\u043C\u0435\u0442\u0440\u043E\u0432\u044B\u0435
      \u043E\u0442\u043C\u0435\u0442\u043A\u0438 \u0434\u0430\u043B\u044C\u043D\u043E\u0441\u0442\u0438
      \u0438 30 \u0433\u0440\u0430\u0434\u0443\u0441\u043D\u044B\u0435 \u043E\u0442\u043C\u0435\u0442\u043A\u0438
