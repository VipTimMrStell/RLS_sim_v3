    Text_Learnihg_for_Block: "15.4.5.   \u041F\u0440\u043E\u0432\u0435\u0440\u044C\u0442\u0435
      \u0440\u0435\u0436\u0438\u043C \u0440\u0430\u0431\u043E\u0442\u044B \u042D\u0421\u0423,
      \u0434\u043B\u044F \u0447\u0435\u0433\u043E \u0432\u043A\u043B\u044E\u0447\u0438\u0442\u0435
      \u043A\u043D\u043E\u043F\u043A\u0443 \u041C\u0415\u0421\u0422\u041D. \u0412\u041A\u041B.
      \u0412\u042B\u0421\u041E\u041A\u041E\u0413\u041E \u043D\u0430 \u043B\u0438\u0446\u0435\u0432\u043E\u0439
      \u043F\u0430\u043D\u0435\u043B\u0438 \u0431\u043B\u043E\u043A\u0430 \u0423\u0412-72
      \u0438 \u043F\u0440\u0438\u0431\u043E\u0440\u043E\u043C \u04264353 (\u0438\u043C\u0435\u0435\u0442\u0441\u044F
      \u0432 \u0417\u0418\u041B\u0435) \u0438\u0437\u043C\u0435\u0440\u044C\u0442\u0435
      \u043D\u0430\u043F\u0440\u044F\u0436\u0435\u043D\u0438\u0435 \u043D\u0430 \u043A\u043E\u043D\u0442\u0440\u043E\u043B\u044C\u043D\u044B\u0445
      \u0433\u043D\u0435\u0437\u0434\u0430\u0445 \u0431\u043B\u043E\u043A\u0430 \u0423\u0412-72.
      \u0432\u0435\u043B\u0438\u0447\u0438\u043D\u044B \u044D\u0442\u0438\u0445 \u043D\u0430\u043F\u0440\u044F\u0436\u0435\u043D\u0438\u0439
      \u0434\u043E\u043B\u0436\u043D\u044B \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u043E\u0432\u0430\u0442\u044C
      \u0443\u043A\u0430\u0437\u0430\u043D\u043D\u044B\u043C \u0432 \u0442\u0430\u0431\u043B\u0438\u0446\u0435
      \u043D\u0430 \u043B\u0438\u0446\u0435\u0432\u043E\u0439 \u043F\u0430\u043D\u0435\u043B\u0438
      \u0431\u043B\u043E\u043A\u0430 \u0423\u0412-72. \u041F\u043E\u043A\u0430\u0437\u0430\u043D\u0438\u044F
      \u0438\u0437\u043C\u0435\u0440\u0438\u0442\u0435\u043B\u044C\u043D\u044B\u0445
      \u043F\u0440\u0438\u0431\u043E\u0440\u043E\u0432 \u043D\u0430 \u043B\u0438\u0446\u0435\u0432\u043E\u0439
      \u043F\u0430\u043D\u0435\u043B\u0438 \u0431\u043B\u043E\u043A\u0430 \u0423\u0412-72
      \u0442\u0430\u043A\u0436\u0435 \u0434\u043E\u043B\u0436\u043D\u044B \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u043E\u0432\u0430\u0442\u044C
      \u044D\u0442\u043E\u0439 \u0442\u0430\u0431\u043B\u0438\u0446\u0435. \u041F\u0440\u0438
      \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u043A\u0438 \u043D\u0430\u043F\u0440\u044F\u0436\u0435\u043D\u0438\u0439
      \u043F\u0438\u0442\u0430\u043D\u0438\u044F \u043D\u0435 \u0434\u043E\u043F\u0443\u0441\u043A\u0430\u0442\u044C
      \u043E\u0442\u043A\u043B\u043E\u043D\u0435\u043D\u0438\u0439 \u043E\u0442 \u0437\u0430\u0434\u0430\u043D\u043D\u044B\u0445
      \u043F\u043E \u043F\u0430\u0441\u043F\u043E\u0440\u0442\u0443 \u043D\u0430
      \u042D\u0421\u0423 \u043F\u0440\u0435\u0434\u0435\u043B\u043E\u0432.\r\n\n\u041F\u0440\u0438\u043C\u0435\u0447\u0430\u043D\u0438\u0435.
      \u041F\u0440\u0438 \u0437\u0430\u043C\u0435\u043D\u0435 \u042D\u0421\u0423
      \u0432\u043D\u0435\u0441\u0442\u0438 \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044E\u0449\u0438\u0435
      \u043F\u0430\u0441\u043F\u043E\u0440\u0442\u043D\u044B\u0435 \u0434\u0430\u043D\u043D\u044B\u0435
      \u0432 \u0442\u0430\u0431\u043B\u0438\u0446\u0443 \u0431\u043B\u043E\u043A\u0430
      \u0423\u0412-72.\r\n"
