    Text_Learnihg_for_Block: "\r\r2. \u041D\u0430 \u0431\u043B\u043E\u043A\u0435
      0-71 \u0432\u043A\u043B\u044E\u0447\u0438\u0442\u0435 \u0442\u0443\u043C\u0431\u043B\u0435\u0440
      \u0412\u041A\u041B. \u0410\u0417\u0418\u041C. \u0421\u0422\u0420\u041E\u0411,
      \u0438 \u0440\u0443\u0447\u043A\u0430\u043C\u0438 \u0417\u0410-\u0414\u0415\u0420\u0416\u041A\u0410
      I, \u0417\u0410\u0414\u0415\u0420\u0416\u041A\u0410 2, \u0414\u041B\u0418\u0422.
      1, \u0414\u041B\u0418\u0422. 2 \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435
      \u043D\u0430\u0447\u0430\u043B\u043E \u0438 \u0434\u043B\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0441\u0442\u044C
      \u0441\u0442\u0440\u043E\u0431\u043E\u0432, \u0430 \u0440\u0443\u0447\u043A\u0430\u043C\u0438
      \u0421\u0415\u041A\u0422\u041E\u0420 I \u0438 \u0421\u0415\u041A\u0422\u041E\u0420
      2 \u043F\u0440\u043E\u0442\u044F\u0436\u0435\u043D\u043D\u043E\u0441\u0442\u044C
      \u0441\u0442\u0440\u043E\u0431\u043E\u0432 \u043F\u043E \u0430\u0437\u0438\u043C\u0443\u0442\u0443.\n\r\n\u041F\u0440\u0438
      \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u043A\u0435 \u043F\u0440\u043E\u0442\u044F\u0436\u0435\u043D\u043D\u043E\u0441\u0442\u0438
      \u043F\u043E \u0430\u0437\u0438\u043C\u0443\u0442\u0443 \u0438\u043C\u0435\u0442\u044C
      \u0432 \u0432\u0438\u0434\u0443, \u0447\u0442\u043E \u0441\u0442\u0440\u0435\u043B\u043A\u0438,
      \u0440\u0430\u0441\u043F\u043E\u043B\u043E\u0436\u0435\u043D\u043D\u044B\u0435
      \u0431\u043B\u0438\u0436\u0435 \u043A \u0448\u043A\u0430\u043B\u0435, \u0443\u043A\u0430\u0437\u044C\u044E\u0430\u044E\u0442
      \u0430\u0437\u0438\u043C\u0443\u0442 \u043D\u0430\u0447\u0430\u043B\u0430 \u0441\u0435\u043A\u0442\u043E\u0440\u0430
      \u0438 \u0434\u043E\u043B\u0436\u043D\u044B \u0432\u0441\u0435\u0433\u0434\u0430
      \u043E\u0442\u0441\u0442\u0430\u0432\u0430\u0442\u044C \u043E\u0442 \u0432\u0442\u043E\u0440\u043E\u0439
      \u0441\u0442\u0440\u0435\u043B\u043A\u0438, \u0435\u0441\u043B\u0438 \u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C
      \u043F\u043E \u0447\u0430\u0441\u043E\u0432\u043E\u0439 \u0441\u0442\u0440\u0435\u043B\u043A\u0435.\r\r"
