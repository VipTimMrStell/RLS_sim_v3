    \u043C\u043E\u043D\u0442\u0430\u0436\u0430.\r\n\u041B\u0438\u0446\u0435\u0432\u0430\u044F
    \u043F\u0430\u043D\u0435\u043B\u044C, \u0432\u043A\u043B\u044E\u0447\u0430\u0435\u0442:\r\n10
