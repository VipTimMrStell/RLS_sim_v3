      \u0421\u0415\u0422\u042C. \u0423\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435
      \u0432\u044B\u0445\u043E\u0434\u043D\u043E\u0435 \u043D\u0430\u043F\u0440\u044F\u0436\u0435\u043D\u0438\u0435
      +27 \u0412 \u043F\u043E \u043F\u0440\u0438\u0431\u043E\u0440\u0443 \u043D\u0430
      \u043B\u0438\u0446\u0435\u0432\u043E\u0439 \u043F\u0430\u043D\u0435\u043B\u0438
      \u0431\u043B\u043E\u043A\u0430. \u041F\u0440\u043E\u0432\u0435\u0440\u044C\u0442\u0435
      \u0440\u0430\u0431\u043E\u0442\u0443 \u0430\u043F\u043F\u0430\u0440\u0430\u0442\u0443\u0440\u044B
      \u0410\u0421\u041F\u0414 \u0441\u043E\u0433\u043B\u0430\u0441\u043D\u043E \u0438\u043D\u0441\u0442\u0440\u043A\u0443\u043A\u0446\u0438\u0438
      \u043F\u043E \u044D\u043A\u0441\u043F\u043B\u0443\u0430\u0442\u0430\u0446\u0438\u0438
      \u043D\u0430 \u0430\u043F\u043F\u0430\u0440\u0430\u0442\u0443\u0440\u0443.\r\n\u0420\u0435\u0437\u0443\u043B\u044C\u0442\u0430\u0442\u044B
      \u043F\u0440\u043E\u0432\u0435\u0434\u0435\u043D\u043D\u043E\u0439 \u043E\u0446\u0435\u043D\u043A\u0438
      \u0433\u043E\u0442\u043E\u0432\u043D\u043E\u0441\u0442\u0438 \u0430\u043F\u043F\u0430\u0440\u0430\u0442\u0443\u0440\u044B
      \u043A \u0431\u043E\u0435\u0432\u043E\u0439 \u0440\u0430\u0431\u043E\u0442\u0435
      \u0434\u043E\u043A\u043B\u0430\u0434\u044B\u0432\u0430\u044E\u0442\u0441\u044F
      \u043D\u0430 \u043A\u043E\u043C\u0430\u043D\u0434\u043D\u044B\u0439 \u043F\u0443\u043D\u043A\u0442.\r\n\u0415\u0441\u043B\u0438
      \u043F\u0440\u0438 \u043F\u0440\u043E\u0432\u0435\u0440\u043A\u0435 \u0431\u0443\u0434\u0435\u0442
      \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u043B\u0435\u043D\u043E, \u0447\u0442\u043E
      \u0431\u043B\u043E\u043A\u0438 \u0440\u0430\u0431\u043E\u0442\u0430\u044E\u0442
      \u043D\u0435\u043D\u043E\u0440\u043C\u0430\u043B\u044C\u043D\u043E, \u0430
      \u0440\u0435\u0433\u0443\u043B\u0438\u0440\u043E\u0432\u043A\u0430\u043C\u0438,
      \u0443\u043A\u0430\u0437\u0430\u043D\u043D\u044B\u043C\u0438 \u0432 \u043D\u0430\u0441\u0442\u043E\u044F\u0449\u0435\u043C
      \u0440\u0430\u0437\u0434\u0435\u043B\u0435, \u043D\u0430\u0441\u0442\u0440\u043E\u0438\u0442\u044C
      \u0431\u043B\u043E\u043A\u0438 \u043D\u0435 \u0443\u0434\u0430\u0435\u0442\u0441\u044F,
      \u043D\u0435\u043E\u0431\u0445\u043E\u0434\u0438\u043C\u043E \u043F\u0440\u043E\u0438\u0437\u0432\u0435\u0441\u0442\u0438
      \u043D\u0430\u0441\u0442\u0440\u043E\u0439\u043A\u0443 \u0438 \u0440\u0435\u0433\u0443\u043B\u0438\u0440\u043E\u0432\u043A\u0443
      \u0430\u043F\u043F\u0430\u0440\u0430\u0442\u0443\u0440\u044B \u0441\u0442\u0430\u043D\u0446\u0438\u0438
      \u0432 \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438
      \u0441 \u043F\u0430\u0440\u0430\u0433\u0440\u0430\u0444\u043E\u043C 17 \u2014
      \xAB\u0418\u0437\u043C\u0435\u0440\u0435\u043D\u0438\u0435 \u043F\u0430\u0440\u0430\u043C\u0435\u0442\u0440\u043E\u0432
      \u0438 \u0440\u0435\u0433\u0443\u043B\u0438\u0440\u043E\u0432\u043A\u0430 \u0441\u0442\u0430\u043D\u0446\u0438\u0438,
      \u0441\u0438\u0441\u0442\u0435\u043C, \u0431\u043B\u043E\u043A\u043E\u0432
      \u0438 \u043C\u0435\u0445\u0430\u043D\u0438\u0437\u043C\u043E\u0432 \u0420\u041B\u0421
      1\u0420\u041B134\u0428\xBB.\r\n\u041F\u043E\u0434\u0441\u0442\u0440\u043E\u0439\u043A\u0443
      \u0438 \u0440\u0435\u0433\u0443\u043B\u0438\u0440\u043E\u0432\u043A\u0443 \u0441\u043B\u0435\u0434\u0443\u0435\u0442
      \u043D\u0430\u0447\u0438\u043D\u0430\u0442\u044C \u043D\u0435 \u0440\u0430\u043D\u0435\u0435,
      \u0447\u0435\u043C \u0447\u0435\u0440\u0435\u0437 30 \u043C\u0438\u043D\u0443\u0442
      \u043F\u043E\u0441\u043B\u0435 \u043F\u043E\u043B\u043D\u043E\u0433\u043E \u0432\u043A\u043B\u044E\u0447\u0435\u043D\u0438\u044F
      \u0441\u0442\u0430\u043D\u0446\u0438\u0438.\r\n\u041F\u043E \u043E\u043A\u043E\u043D\u0447\u0430\u043D\u0438\u0438
      \u043F\u0440\u043E\u0432\u0435\u0440\u043A\u0438 \u0430\u043F\u043F\u0430\u0440\u0430\u0442\u0443\u0440\u044B
      \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435 \u043D\u0430 \u0431\u043B\u043E\u043A\u0435
      \u0415-71 \u0442\u0443\u043C\u0431\u043B\u0435\u0440 \u0414\u0418\u0421\u0422\u0410\u041D\u0426\u0418\u041E\u041D\u041D\u041E-\u0410\u0412\u0422\u041E\u041D\u041E\u041C\u041D\u041E
      \u0432 \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u0414\u0418\u0421\u0422\u0410\u041D\u0426\u0418\u041E\u041D\u041D\u041E;
      \u043D\u0430 \u0431\u043B\u043E\u043A\u0435 \u041A-71 \u0442\u0443\u043C\u0431\u043B\u0435\u0440
      \u041C\u0415\u0421\u0422\u041D.-\u0414\u0418\u0421\u0422\u0410\u041D\u0426.
      \u0432 \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u0414\u0418\u0421\u0422\u0410\u041D\u0426.\r"
