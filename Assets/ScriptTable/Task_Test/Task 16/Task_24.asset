    Text_Learnihg_for_Block: "2.   \u0423\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435
      \u043D\u0430 \u043F\u0443\u043B\u044C\u0442\u0435 \u041F\u041E\u0412-71 \u043F\u0435\u0440\u0435\u043A\u043B\u044E\u0447\u0430\u0442\u0435\u043B\u044C
      \u041C\u0410\u0420\u041A\u0415\u0420 \u0432 \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435
      \u0412\u041A\u041B.\r\n\n3.   \u041D\u0430\u0436\u043C\u0438\u0442\u0435 \u043D\u0430
      \u043F\u0443\u043B\u044C\u0442\u0435 \u041F\u041E\u0412-71 \u043A\u043D\u043E\u043F\u043A\u0443
      \u0417\u0412\u0423\u041A\u041E\u0412\u041E\u0419 \u0421\u0418\u0413\u041D\u0410\u041B,
      \u043F\u043E\u0441\u043B\u0435 \u0447\u0435\u0433\u043E \u0441\u0440\u0430\u0431\u0430\u0442\u044B\u0432\u0430\u0435\u0442
      \u0437\u0432\u0443\u043A\u043E\u0432\u043E\u0439 \u0441\u0438\u0433\u043D\u0430\u043B
      \u0432 \u0432\u044B\u0441\u043E\u0442\u043E\u043C\u0435\u0440\u0435.\r\n\u0421
      \u0432\u044B\u0441\u043E\u0442\u043E\u043C\u0435\u0440\u0430 \u043F\u043E\u0441\u0442\u0443\u043F\u0430\u0435\u0442
      \u0441\u0438\u0433\u043D\u0430\u043B \u043E \u0433\u043E\u0442\u043E\u0432\u043D\u043E\u0441\u0442\u0438
      \u0435\u0433\u043E \u043A \u0440\u0430\u0431\u043E\u0442\u0435, \u0432 \u0440\u0435\u0437\u0443\u043B\u044C\u0442\u0430\u0442\u0435
      \u0447\u0435\u0433\u043E \u043D\u0430 \u043F\u0443\u043B\u044C\u0442\u0435
      \u041F\u041E\u0412-71 \u0434\u043E\u043B\u0436\u043D\u0430 \u0437\u0430\u0433\u043E\u0440\u0435\u0442\u044C\u0441\u044F
      \u043B\u0430\u043C\u043F\u0430 \u0413\u041E\u0422\u041E\u0412\u041D\u041E\u0421\u0422\u042C.\r\n\n4.
      \u0420\u0443\u0447\u043A\u043E\u0439 \u0423\u0421\u0422\u0410\u041D\u041E\u0412\u041A\u0410
      \u0410\u0417\u0418\u041C\u0423\u0422\u0410 \u043D\u0430 \u043F\u0443\u043B\u044C\u0442\u0435
      \u041F\u041E\u0412-71 \u043F\u043E\u0434\u0432\u0435\u0434\u0438\u0442\u0435
      \u044D\u043B\u0435\u043A\u0442\u0440\u043E\u043D\u043D\u044B\u0439 \u043C\u0430\u0440\u043A\u0435\u0440
      \u043A \u0446\u0435\u043B\u0438, \u0432\u044B\u0441\u043E\u0442\u0443 \u043A\u043E\u0442\u043E\u0440\u043E\u0439
      \u043D\u0430\u0434\u043E \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0438\u0442\u044C,
      \u0438 \u0443\u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0435 \u0442\u0443\u043C\u0431\u043B\u0435\u0440
      \u0414\u0410\u042E \u0426\u0423 - \u0412\u042B\u0421\u041E\u0422\u0410 \u041F\u0420\u0418\u041D\u042F\u0422\u0410
      \u0432 \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u0414\u0410\u042E
      \u0426\u0423.  \u041F\u043E\u0441\u043B\u0435 \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0435\u043D\u0438\u044F
      \u0432\u044B\u0441\u043E\u0442\u044B \u0446\u0435\u043B\u0435\u0439 \u0441
      \u043F\u043E\u043C\u043E\u0449\u044C\u044E \u0432\u044B\u0441\u043E\u0442\u043E\u043C\u0435\u0440\u0430
      \u0432\u043A\u043B\u044E\u0447\u0438\u0442\u0441\u044F \u043B\u0430\u043C\u043F\u0430
      \u043F\u043E\u0434\u0441\u0432\u0435\u0442\u0430 \u0448\u043A\u0430\u043B\u044B
